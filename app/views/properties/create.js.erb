<% if current_user.status == "admin" %>
	window.location.href="/properties"
<% else %>
	$($("ul li a span")[0]).parent().removeClass("active");
	$($("ul li a span")[1]).parent().addClass("active");
	$( ".payment" ).after( "<%= escape_javascript( render( :partial => "payment_form", :locals => { } ) ) %>" );
	$(".payment").remove();
<% end %>