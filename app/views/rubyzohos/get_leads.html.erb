<!-- <div class="container"> -->
    <table>
      <thead>
      <tr>
        <th>Lead Owner</th>
        <th>Company</th>
        <th>Firstname</th>
        <th>Lastname</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Fax</th>
        <th>Mobile</th>
        <th>Lead Source</th>
        <th>Lead Status</th>
        <th>Industry</th>
        <th>Annual Revenue</th>
        <th>Rating</th>
        <th>Secondary Email</th>
        <th>Twitter</th>
        <th>Street</th>
        <th>City</th>
        <th>Website</th>
        <th>Province</th>
        <th>Postal Code</th>
        <th>Country</th>
        <th>Description</th>
        <th colspan="3"></th>
      </tr>
      </thead>
      <tbody>
      <% @le.each do |le| %>
          <tr>
            <td><%= le.lead_owner %></td>
            <td><%= le.company %></td>
            <td><%= le.first_name %></td>
            <td><%= le.last_name %></td>
            <td><%= le.email %></td>
            <td><%= le.phone %></td>
            <td><%= le.fax %></td>
            <td><%= le.mobile %></td>
            <td><%= le.lead_source %></td>
            <td><%= le.lead_status %></td>
            <td><%= le.industry %></td>
            <td><%= le.annual_revenue %></td>
            <td><%= le.rating %></td>
            <td><%= le.secondary_email %></td>
            <td><%= le.twitter %></td>
            <td><%= le.street %></td>
            <td><%= le.city %></td>
            <td><%= le.website %></td>
            <td><%= le.state %></td>
            <td><%= le.zip_code %></td>
            <td><%= le.country %></td>
            <td><%= le.description %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
<!-- </div>     -->