<!-- <div class="container"> -->
    <table>
      <thead>
      <tr>
        <th>Account Name</th>
        <th>Lead Source</th>
        <th>Contact Owner</th>
        <th>Title</th>
        <th>Firstname</th>
        <th>Lastname</th>
        <th>Email</th>
        <th>Department</th>
        <th>Mobile</th>
        <th>Phone</th>
        <th>Home phone</th>
        <th>Other phone</th>
        <th>Assistant</th>
        <th>Asst phone</th>
        <th>Fax</th>
        <th>Reports To</th>
        <th>Secondary Email</th>
        <th>Twitter</th>
        <th>Mailing Street</th>
        <th>Other Street</th>
        <th>Mailing City</th>
        <th>Other City</th>
        <th>Mailing Province</th>
        <th>Other Province</th>
        <th>Mailing Postal Code</th>
        <th>Other Postal Code</th>
        <th>Mailing Country</th>
        <th>Other Country</th>
        <th>Description</th>
        <th colspan="3"></th>
      </tr>
      </thead>
      <tbody>
      <% @co.each do |co| %>
          <tr>
            <td><%= co.account_name %></td>
            <td><%= co.lead_source %></td>
            <td><%= co.contact_owner %></td>
            <td><%= co.title %></td>
            <td><%= co.first_name %></td>
            <td><%= co.last_name %></td>
            <td><%= co.email %></td>
            <td><%= co.department %></td>
            <td><%= co.mobile %></td>
            <td><%= co.phone %></td>
            <td><%= co.home_phone %></td>
            <td><%= co.other_phone %></td>
            <td><%= co.assistant %></td>
            <td><%= co.asst_phone %></td>
            <td><%= co.reports_to %></td>
            <td><%= co.fax %></td>
            <td><%= co.secondary_email %></td>
            <td><%= co.twitter %></td>
            <td><%= co.mailing_street %></td>
            <td><%= co.other_street %></td>
            <td><%= co.mailing_city %></td>
            <td><%= co.other_city %></td>
            <td><%= co.mailing_state %></td>
            <td><%= co.other_state %></td>
            <td><%= co.mailing_zip %></td>
            <td><%= co.other_zip %></td>
            <td><%= co.mailing_country %></td>
            <td><%= co.other_country %></td>
            <td><%= co.description %></td>
          </tr>
      <% end %>
      </tbody>
    </table>    
