                    <img src="assets/properties/slider/fslider/p1.jpg" id="bg" alt="main-title"/>
                    <div class="container">
                        <div class="panel">
                            <h1>Contact</h1>    
                            <p>Sub Title Here</p>    
                        </div>
                    </div>
                </div> 
                <!--    News Carousel-->
                <div class="news-carousel-wrapper">
                    <dl id="news-carousel" class="news-carousel-style">
                      <%= render 'properties/news_carousel'%>
                    </dl>
                </div>
                <!--    End News Carousel-->
                <div class="container">
                    <div class="row-fluid">
                        <div class="span3">
                            <div class="panel ">
                                <h2 class="main-heading bottom-line line-after"><span class="main-circle-icon"><i class="icon-check"></i></span> We Are Here</h2>   
                                <p>
                                    Proper Realestate<br>
                                    Marketing Dept<br>
                                    12-123 1/2 Rodaria SE<br>
                                    Toronto ON M1B2P4<br>
                                    Paris
                                </p>
                                <strong>Phone:</strong> +1 1800.43.989 <br>  
                                <strong>Fax:</strong> 416.556.4657 <br>
                                <strong>Email:</strong> <a href="mailto:someone@email.com"> someone@company.com</a>
                            </div>
                        </div>
                        <div class="span9">
                            <div class="panel left-line">
                                <h3 class="bottom-line main-heading box-panel"><span class="main-circle-icon"><i class="icon-map-marker"></i></span> Location</h3>
                                <iframe id="map"  width="100%" height="350" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?f=q&amp;source=s_q&amp;hl=id&amp;geocode=&amp;q=arvada&amp;aq=&amp;sll=39.777936,-105.074272&amp;sspn=0.149605,0.338173&amp;ie=UTF8&amp;hq=&amp;hnear=Arvada,+Jefferson,+Colorado&amp;ll=39.802764,-105.087484&amp;spn=0.299124,0.676346&amp;t=m&amp;z=11&amp;output=embed"></iframe>    
                                <h3 class="bottom-line main-heading box-panel"><span class="main-circle-icon"><i class="icon-envelope"></i></span> Feel free to send us a message</h3>
                                <form>
                                    <div class="box-panel ">
                                        <div class="row-fluid">
                                            <div class="span6">
                                                <label for="first_name" accesskey="U">First Name*</label>
                                                <input type="text" class="input-block-level" name="first_name" value="" id="first_name" placeholder="First Name"/>
                                            </div>
                                            <div class="field span6">
                                                <label for="last_name" accesskey="O">Last Name*</label>
                                                <input type="text"  class="input-block-level" name="last_name" value="" id="last_name" placeholder="Last Name">
                                            </div>
                                        </div>
                                        <div class="row-fluid">
                                            <div class="field span6">
                                                <label for="email" accesskey="E">Email*</label>
                                                <input type="email" class="input-block-level" name="email" value="" id="email" placeholder="your_email@address.com"/>
                                            </div>
                                            <div class="field span6">
                                                <label for="phone" accesskey="P">Contact Number</label>
                                                <input type="tel" class="input-block-level" name="phone" value="" id="phone" placeholder="( 000 ) 000 - 0000">
                                            </div>
                                        </div>
                                        <div class="row-fluid">
                                            <label for="comments" accesskey="C">Comments*</label>
                                            <textarea id="comments" rows="4" class="input-block-level" name="comments" placeholder="Please type your comments here.."></textarea>
                                        </div>
                                        <div class="row-fluid">
                                            <label for="verify" accesskey="V">Verify*</label>
                                            <input name="verify" type="text" id="verify" size="6" value="" placeholder="verify code">
                                            <label><img src="assets/captcha.png" alt="Image verification" border="0" width="220"></label>
                                        </div>
                                        <br/>
                                        <button type="submit" class="btn-proper btn " > Send <i class="icon-arrow-right"></i></button>
                                    </div>

                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>            
