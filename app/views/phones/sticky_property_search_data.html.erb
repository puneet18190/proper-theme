<table class="sticky_search_data table" style="width: 75%">
	<tr>
		<th></th>
		<th>Landlord</th>
		<th>Tenant</th>
	</tr>

	<tr>
		<td>Name</td>
		<td><%= "#{@landlord.try('first_name')} #{@landlord.try('last_name')}" %></td>
		<td><%= "#{@tenant.try('first_name')} #{@tenant.try('last_name')}" %></td>
	</tr>
	<tr>
		<td>Mobile</td>
		<td class="example-p-2"><a href="#"><%= @landlord.try('mobile') %></a></td>
		<td class="example-p-2"><a href="#"><%= @tenant.try('mobile') %></a></td>
	</tr>
	<tr>
		<td>Alternative Phone</td>
		<td class="example-p-2"><a href="#"><%= @landlord.try('phone') %></a></td>
		<td class="example-p-2"><a href="#"><%= @tenant.try('phone') %></a></td>
	</tr>
	<tr>
		<td>Email</td>
		<td><%= mail_to @landlord.try('email') %></td>
		<td><%= mail_to @tenant.try('email') %></td>
	</tr>
	<tr>
		<td></td>
		<td><%unless @landlord.blank?%><a class='btn btn-proper' href='/landlords/<%= @landlord.id %>/edit '>Go to Record</a><%end%></td>
		<td><%unless @tenant.blank?%><a class='btn btn-proper' href='/tenants/<%= @landlord.id %>/edit '>Go to Record</a><%end%></td>
	</tr>

</table>