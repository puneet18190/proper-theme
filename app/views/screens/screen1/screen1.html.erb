
<script type="text/javascript">
$(document).ready(function() {
	var index=0;
	run_script(index);
})	

function run_script(index){
	var urls =  ["typograpy","sing_up","upload_step1","upload_step2","upload_step3","screens/screen2/page1","screens/screen/page2"];
	$.ajax({
		type : 'get',
		url : '/'+urls[index],
		async: false,
		success : function(response){
			$(".main_container").html(response);
			$(".screen_num").html(index+1)
			index = index+1;
			if (index == urls.length){
				index = 0;
			}
			setTimeout(
			  function() 
			  {
			    run_script(index);
			  }, 10000);
			
		},
		error : function(){
			run_script(0);
		}
	})
}
</script>