            <!--Start Content-->
<div id="content">
  <%= render :partial => "/layouts/main_title",:locals => { :maintitle => "Property Detail", :subtitle=>"Newest Addition"} %>
  
  <div class="container" style="width:auto;height:auto">
    
    <div class="row-fluid">
      <% unless @advertisements_l.blank? %>
      <div class="ad_left" style="width:332px;height:500px;margin:0;margin-top:0px;float:left">    
        <% @advertisements_l.each do |ad| %>
          <% if (ad.expiry_date - (Time.now).to_date)> 0 %>
            <div id="oval_left" style="margin:5px;padding:3px;width:300px;border-radius:0;height:300px;border: 1px solid">
              <b><%= ad.name%></b><br/>
              <%= ad.email%><br/>
              <%= ad.description%><br/>
              <%= image_tag ad.image.url,:style=>"width:190px"%><br/>
              Contact:<br/>
              <%= ad.phone%><br/>
              <%#= ad.expiry_date%><br/>
            </div>
         <% end %>
        <% end %>  
      </div>
    <% end %>
    <% unless @advertisements_m.blank? %>
      <div class="ad_right" style="width:332px;height:500px;margin:0;margin-left:320px;margin-top:0px;position:absolute">    
        <% @advertisements_m.each_with_index do |ad| %>
          <% if (ad.expiry_date - (Time.now).to_date)> 0 %>
            <div id="oval_right" style="margin:5px;padding:3px;width:300px;border-radius:0;height:300px;border: 1px solid">
              <b><%= ad.name%> </b><br>
              <%= ad.email%><br>
              <%= ad.description%><br>
              <%= image_tag ad.image.url,:style=>"width:190px"%><br>
              Contact:
              <%= ad.phone%><br>
              <%#= ad.expiry_date%><br>
            </div>
          <% end %>
        <% end %>      
      </div>
    <% end %>
    <% unless @advertisements_r.blank? %>
      <div class="ad_right" style="width:332px;height:500px;margin:0;margin-left:640px;margin-top:0;position:absolute">    
        <% @advertisements_r.each_with_index do |ad| %>
          <% if (ad.expiry_date - (Time.now).to_date)> 0 %>
            <div id="oval_right" style="margin:5px;padding:3px;width:300px;border-radius:0;height:300px;border: 1px solid">
              <b><%= ad.name%> </b><br>
              <%= ad.email%><br>
              <%= ad.description%><br>
              <%= image_tag ad.image.url,:style=>"width:190px"%><br>
              Contact:
              <%= ad.phone%><br>
              <%#= ad.expiry_date%><br>
            </div>
          <% end %>
        <% end %>      
      </div>
    <% end %>
    <% unless @advertisements_x.blank? %>
      <div class="ad_right" style="width:332px;height:500px;margin:0;margin-left:960px;margin-top:0;float:right;position:absolute">    
        <% @advertisements_x.each_with_index do |ad| %>
          <% if (ad.expiry_date - (Time.now).to_date)> 0 %>
            <div id="oval_right" style="margin:5px;padding:3px;width:300px;border-radius:0;height:300px;border: 1px solid">
              <b><%= ad.name%> </b><br>
              <%= ad.email%><br>
              <%= ad.description%><br>
              <%= image_tag ad.image.url,:style=>"width:190px"%><br>
              Contact:
              <%= ad.phone%><br>
              <%#= ad.expiry_date%><br>
            </div>
          <% end %>
        <% end %>      
      </div>
    <% end %>
    </div>
    <div style="margin-top:720px;font-size:xx-large;margin-bottom:50px"><center><b>Enquire inside to advertise here</b></center></div>
  </div>
</div>

</script>
