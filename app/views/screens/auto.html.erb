
<script type="text/javascript">
$(document).ready(function() {
	var urls =  ["typograpy","sing_up","upload_step1","upload_step2","upload_step3","screens/page1","screens/page2"];
		
	$.each(urls, function( index, url ) {
		$.ajax({
			type : 'get',
			url : '/'+url,
			async: false,
			success : function(response){
				$("body").html(response)
			},
			error : function(){
				alert('Sorry, something has gone wrong with the server response.')
			}
		})
	})
})	
</script>