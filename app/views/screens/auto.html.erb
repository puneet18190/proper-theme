
<script type="text/javascript">
$(document).ready(function() {
	var index=0;
	run_script(index);
})	

function run_script(index){
	var urls =  ["typograpy","sing_up","upload_step1","upload_step2","upload_step3","screens/page1","screens/page2"];
	$.ajax({
		type : 'get',
		url : '/'+urls[index],
		async: false,
		success : function(response){
			$(".main_container").html(response);
			index = index+1;
			if (index == urls.length){
				index = 0;
			}
			setTimeout(
			  function() 
			  {
			    run_script(index);
			  }, 10000);
			
		},
		error : function(){
			alert('Sorry, something has gone wrong with the server response.')
		}
	})
}
</script>